{
  "name": "api-behavior",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api-behavior/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/api-behavior/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/api-behavior"
      ],
      "options": {
        "jestConfig": "libs/api-behavior/jest.config.ts",
        "passWithNoTests": true,
        "watch": true
      }
    }
  },
  "tags": []
}
